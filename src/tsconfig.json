{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"lib": [
			"ES2020", "DOM", "WebWorker"
		],
		"strict": true,
		"noImplicitAny": false,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"alwaysStrict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": false,
	},
    "include": [
        "**/*.ts"
    ]
}